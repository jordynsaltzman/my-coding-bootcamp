{
    "name": "my-coding-bootcamp",
    "version": "1.0.0",
    "description": "Save coding resources",
    "main": "server.js",
    "dependencies": {
        "@hapi/joi": "^17.1.1",
        "axios": "^0.19.2",
        "bcryptjs": "^2.4.3",
        "bootstrap": "^4.4.1",
        "concurrently": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.18.2",
        "if-env": "^1.0.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.9",
        "morgan": "^1.10.0",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "nodemon": "^2.0.2"
    },
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jordynsaltzman/my-coding-bootcamp.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jordynsaltzman/my-coding-bootcamp/issues"
    },
    "homepage": "https://github.com/jordynsaltzman/my-coding-bootcamp#readme"
}
